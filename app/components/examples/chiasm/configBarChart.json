{
  "layout": {
    "plugin": "layout",
    "state": {
      "containerSelector": "#_example_chiasm__container",
      "layout": {
        "orientation": "vertical",
        "children": [
          "barChart"
        ]
      }
    }
  },
  "barChart": {
    "plugin": "barChart",
    "state": {
      "orientation": "vertical",
      "barSizeColumn": "frequency",
      "fill": "steelblue",
      "stroke": "none",
      "strokeWidth": "2px",
      "barPadding": 0.2
    }
  },
  "links": {
    "plugin": "links",
    "state": {
      "bindings": [
        "self.data -> barChart.data"
      ]
    }
  }
}
