          "intro.js": "2.0.0"
        }
      },
      "github:enjalot/Inlet@0.1.1": {
        "main": "inlet"
      },
      "github:codemirror/codemirror@5.3.0": {
        "format": "global",
        "directories": {
          "lib": "."
        }
      },
      "github:angular-slider/angularjs-slider@2.4.1": {}
    }
  },
  "xo": {
    "esnext": true,
    "space": 2,
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "app/jspm_packages/**",
      "app/system.config.js",
      "*.js",
      "dataset/**",
      "!dataset/example/**"
    ],
    "rules": {
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "no-warning-comments": 0
    }
  }
