      ".publish/**",
      "dataset/**",
      ".tmp/**",
      "builds/**",
      "jspm.**.js",
      "scripts/*.js"
    ],
    "rules": {
      "no-console": "warn",
      "import/no-extraneous-dependencies": 0,
      "import/namespace": 0,
      "import/named": 0,
      "import/extensions": 0,
      "import/no-unassigned-import": 0,
      "import/no-webpack-loader-syntax": 0,
      "import/no-unresolved": 0,
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "no-warning-comments": [
        1,
        {
          "terms": [
            "todo",
            "fixme",
            "xxx"
          ],
          "location": "start"
        }
      ]
    }
