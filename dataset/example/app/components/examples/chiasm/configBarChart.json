{
  "layout": {
    "plugin": "layout",
    "state": {
      "containerSelector": "#_example_chiasm__container",
      "layout": {
        "orientation": "vertical",
        "children": [
          "barChart"
        ]
      }
    }
  },
  "barChart": {
    "plugin": "barChart",
    "state": {
      "xAxisLabelText": "Letter",
      "xColumn": "letter",
      "yAxisLabelText": "Frequency",
      "yColumn": "frequency",
      "fill": "#1EABE8",
      "stroke": "#008CC8"
    }
  },
  "links": {
    "plugin": "links",
    "state": {
      "bindings": [
        "self.dataset -> barChart.dataset"
      ]
    }
  }
}
